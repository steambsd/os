print_info() {

    info title
    info underline

    prin "OS" "$(cat /etc/motd.safe | grep SteamBSD)"
    info "DE" de
    info "CPU" cpu
    info "GPU" gpu
    info "Memory" memory
    info "Uptime" uptime
    info "Resolution" resolution

     info "CPU Usage" cpu_usage
     info "Local IP" local_ip
     info "Public IP" public_ip

    info cols
}
